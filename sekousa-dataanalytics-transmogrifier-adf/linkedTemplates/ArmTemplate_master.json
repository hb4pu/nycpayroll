{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sekousa-dataanalytics-transmogrifier-adf"
		},
		"ls_adls2_nycpayroll_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls2_nycpayroll'"
		},
		"ls_sqldb_prototype_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sqldb_prototype'"
		},
		"ls_synapse_prototype_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_synapse_prototype'"
		},
		"ls_adls_transmogrify2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_adls_transmogrify2'"
		},
		"ls_adls2_nycpayroll_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sekousadatransmogrifyst.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sekousa-dataanalytics-transmogrifier-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls2_nycpayroll_accountKey": {
						"value": "[parameters('ls_adls2_nycpayroll_accountKey')]"
					},
					"ls_sqldb_prototype_connectionString": {
						"value": "[parameters('ls_sqldb_prototype_connectionString')]"
					},
					"ls_synapse_prototype_connectionString": {
						"value": "[parameters('ls_synapse_prototype_connectionString')]"
					},
					"ls_adls_transmogrify2_connectionString": {
						"value": "[parameters('ls_adls_transmogrify2_connectionString')]"
					},
					"ls_adls2_nycpayroll_properties_typeProperties_url": {
						"value": "[parameters('ls_adls2_nycpayroll_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "sekousa-dataanalytics-transmogrifier-adf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'sekousa-dataanalytics-transmogrifier-adf_ArmTemplate_0')]"
			]
		}
	]
}